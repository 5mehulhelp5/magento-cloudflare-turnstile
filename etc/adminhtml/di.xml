<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="PixelOpen\CloudflareTurnstile\Observer\Validate\Admin">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="actions" xsi:type="array">
                    <item name="login" xsi:type="object">Magento\Backend\Controller\Adminhtml\Index\Index</item>
                    <item name="password" xsi:type="object">Magento\User\Controller\Adminhtml\Auth\Forgotpassword</item>
                </item>
            </argument>
        </arguments>
    </type>
    <preference for="PixelOpen\CloudflareTurnstile\Model\ConfigProviderInterface" type="PixelOpen\CloudflareTurnstile\Model\ConfigProvider\Adminhtml" />
</config>
